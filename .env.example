# OpenRouter API Key (получить на https://openrouter.ai)
OPENROUTER_API_KEY=sk-or-v1-xxxxxxxxxxxxx

# Разрешенные origins для CORS
ALLOWED_ORIGINS=http://localhost:8080,https://your-domain.com

# Опционально: для production
ENVIRONMENT=production
LOG_LEVEL=info

# Опционально: если используете Redis для кеширования
REDIS_URL=redis://redis:6379

# Опционально: для мониторинга
SENTRY_DSN=your_sentry_dsn_here